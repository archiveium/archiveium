{
	"name": "archiveium",
	"version": "0.0.29-beta.15",
	"private": true,
	"scripts": {
		"dev": "vite dev --host",
		"build": "svelte-kit sync && vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest run",
		"test:unit:monitor": "vitest",
		"populate:fake-email-data": "vite-node scripts/populateFakeEmailData.ts",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"release:prod": "release-it --ci --config release-it.json",
		"release:beta": "release-it --ci --config release-it.json --preRelease=beta",
		"release:dry-run": "release-it --ci --config release-it.json --dry-run",
		"typecheck": "tsc",
		"test:coverage": "vitest run --coverage",
		"kysely:codegen": "DATABASE_URL=postgres://archiveium:archiveiumPassword@db/archiveium kysely-codegen --out-file src/lib/server/database/types.d.ts"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "3.749.0",
		"@floating-ui/dom": "1.6.3",
		"@smithy/util-stream": "2.1.1",
		"bullmq": "5.13.2",
		"config": "3.3.12",
		"dompurify": "3.0.9",
		"flowbite": "1.8.1",
		"flowbite-svelte": "0.44.24",
		"handlebars": "4.7.8",
		"imapflow": "1.0.164",
		"ioredis": "5.3.2",
		"jsdom": "24.0.0",
		"kysely": "0.27.5",
		"lodash": "4.17.21",
		"luxon": "3.4.4",
		"mailparser": "3.7.1",
		"meilisearch": "^0.49.0",
		"nodemailer": "6.9.15",
		"p-limit": "4.0.0",
		"pg": "8.12.0",
		"pino": "8.21.0",
		"pino-pretty": "10.3.1",
		"tailwind-merge": "1.14.0",
		"zod": "3.23.8"
	},
	"devDependencies": {
		"@faker-js/faker": "^9.0.3",
		"@playwright/test": "1.50.1",
		"@release-it/bumper": "6.0.1",
		"@sveltejs/adapter-auto": "2.1.1",
		"@sveltejs/adapter-node": "1.3.1",
		"@sveltejs/kit": "1.30.4",
		"@types/config": "3.3.5",
		"@types/dompurify": "3.0.5",
		"@types/imapflow": "1.0.19",
		"@types/jsdom": "21.1.7",
		"@types/lodash": "4.17.15",
		"@types/luxon": "3.4.2",
		"@types/mailparser": "3.4.4",
		"@types/nodemailer": "6.4.15",
		"@types/pg": "8.11.6",
		"@typescript-eslint/eslint-plugin": "5.62.0",
		"@typescript-eslint/parser": "5.62.0",
		"@vitest/coverage-v8": "2.0.2",
		"autoprefixer": "10.4.19",
		"eslint": "8.57.0",
		"eslint-config-prettier": "8.10.0",
		"eslint-plugin-svelte": "2.35.1",
		"husky": "9.0.11",
		"kysely-codegen": "0.11.0",
		"postcss": "8.5.3",
		"postcss-load-config": "4.0.2",
		"prettier": "2.8.8",
		"prettier-plugin-svelte": "2.10.1",
		"release-it": "17.6.0",
		"svelte": "4.2.12",
		"svelte-check": "3.6.4",
		"tailwindcss": "3.4.4",
		"tslib": "2.6.3",
		"typescript": "5.3.3",
		"vite": "4.5.3"
	},
	"type": "module"
}
