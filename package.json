{
	"name": "archiveium",
	"version": "0.0.20-beta.2",
	"private": true,
	"scripts": {
		"dev": "export USE_AS_CRON_PROCESSOR=true && vite dev",
		"build": "svelte-kit sync && vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest run",
		"test:unit:monitor": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"release:prod": "release-it --ci --config release-it.json",
		"release:beta": "release-it --ci --config release-it.json --preRelease=beta",
		"release:dry-run": "release-it --ci --config release-it.json --dry-run",
		"typecheck": "tsc",
		"test:coverage": "vitest run --coverage",
		"kysely:codegen": "DATABASE_URL=postgres://archiveium:archiveiumPassword@db/archiveium kysely-codegen --out-file src/lib/server/database/types.d.ts"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "3.256.0",
		"@floating-ui/dom": "^1.4.5",
		"bullmq": "^4.13.1",
		"config": "^3.3.9",
		"flowbite": "^1.7.0",
		"flowbite-svelte": "^0.40.1",
		"imapflow": "1.0.119",
		"ioredis": "^5.3.1",
		"kysely": "^0.26.1",
		"luxon": "^3.3.0",
		"mailparser": "^3.6.4",
		"nodemailer": "^6.9.1",
		"pg": "^8.11.2",
		"pino": "^8.16.1",
		"pino-pretty": "^10.2.3",
		"tailwind-merge": "^1.14.0",
		"handlebars": "^4.7.7",
		"zod": "^3.20.6",
		"lodash": "4.17.21",
		"p-limit": "4.0.0"
	},
	"devDependencies": {
		"@types/mailparser": "^3.4.0",
		"@types/nodemailer": "^6.4.7",
		"@playwright/test": "^1.28.1",
		"@release-it/bumper": "^4.0.2",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/adapter-node": "^1.3.1",
		"@sveltejs/kit": "^1.5.0",
		"@types/config": "^3.3.0",
		"@types/imapflow": "^1.0.11",
		"@types/luxon": "^3.2.0",
		"@types/pg": "^8.10.2",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"@vitest/coverage-v8": "^0.32.4",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.26.0",
		"kysely-codegen": "^0.10.1",
		"postcss": "^8.4.24",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"release-it": "15.10.3",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.3.0",
		"@types/lodash": "^4.14.191"
	},
	"type": "module"
}
